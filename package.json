{
  "name": "nockable",
  "version": "1.0.0",
  "description": "Nock-style HTTP mocking using either nock or real servers",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register",
    "compile": "babel -d dist/ src/",
    "prepublish": "npm test && npm run compile",
    "build": ":",
    "release": "npm install @wix/wnpm-ci && wnpm-release -- --no-shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/nockable.git"
  },
  "keywords": [
    "nockable",
    "nock",
    "real-nock"
  ],
  "author": "Wix Restaurants",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wix/nockable/issues"
  },
  "homepage": "https://github.com/wix/nockable",
  "dependencies": {
    "q": "^1.4.1",
    "nock": "^9.0.13",
    "real-nock": "git://github.com/TabDigital/real-nock.git"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "babel-runtime": "^6.6.1",
    "chai": "^2.2.0",
    "mocha": "^2.2.1",
    "axios": "^0.16.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "directories": {
    "test": "test"
  }
}
